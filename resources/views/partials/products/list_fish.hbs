
{{#each this}}
<div class="item">
    <div class="img">
        <img src="./image/cavang.png" alt="">
    </div>
    <div class="info">
    <div class="info_row">
        <Label>{{this.name}}</Label>
    </div>
    <div  class="info_row">
        <Label >{{this.price}}</Label>
    </div>
    <div  class="info_row">
        <button class="btn">Add to cart</button>
    </div>
    </div>
</div>
{{/each}}
